{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "sourceMap": true,
    "jsx": "react",
    "lib": ["dom", "es2016"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}